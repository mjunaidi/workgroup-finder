<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>/angularjs/*</url-pattern>
    <url-pattern>/angularjs/jquery-1.10.2.min.map</url-pattern>
    <url-pattern>/jquery-1.10.2.min.map</url-pattern>
    <url-pattern>/assets/*</url-pattern>
    <url-pattern>/bootstrap/*</url-pattern>
    <url-pattern>/css/*</url-pattern>
    <url-pattern>/js/*</url-pattern>
    <url-pattern>/data/*</url-pattern>
    <url-pattern>/content/*</url-pattern>
    <url-pattern>/contentdata/*</url-pattern>
  </servlet-mapping>
</web-app>